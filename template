const ll INF = 1e18;
const int MOD = 1e9 + 7;

int sum(int a, int b) {
    a += b;
    if (a >= MOD) {
        a -= MOD;
    }
    return a;
}

int sub(int a, int b) {
    a -= b;
    if (a < 0) {
        a += MOD;
    }
    return a;
}

int mul(int a, int b) {
    return (long long)a * b % MOD;
}

